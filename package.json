  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./src -s --copy-files",
    "start": "NODE_PATH=./build node ./build/index.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "NODE_PATH=./src npm run clean && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline --copy-files",
    "dev": "NODE_PATH=./src API_ENDPOINT=\"http://endpoint_from_env.com\" nodemon --exec ts-node src/index.ts",
    "dev:window": "cross-env NODE_PATH=./src API_ENDPOINT=\"http://endpoint_from_env.com\" nodemon --exec ts-node src/index.ts"
  },
