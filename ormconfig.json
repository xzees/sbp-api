{
  "type": "mysql",
  "host": "DB_HOST",
  "port": "DB_PORT",
  "username": "DB_USER",
  "password": "DB_PASS",
  "database": "DB_NAME",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/database/entity/**/*.ts"
  ],
  "migrations": [
    "src/database/migration/**/*.ts"
  ],
  "subscribers": [
    "src/database/subscriber/**/*.ts"
  ]
}